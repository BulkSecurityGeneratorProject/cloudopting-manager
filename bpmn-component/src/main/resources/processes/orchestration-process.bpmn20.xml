    </intermediateCatchEvent>
    <sequenceFlow id="flow17" sourceRef="timerChkIp" targetRef="chkAquireIp"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway10" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="noTesting" sourceRef="exclusivegateway10" targetRef="vmTask">
