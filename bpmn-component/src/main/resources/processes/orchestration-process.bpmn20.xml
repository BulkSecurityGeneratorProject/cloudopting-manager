    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway11" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow20" sourceRef="exclusivegateway11" targetRef="r10kTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isTesting==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="exclusivegateway11" targetRef="theEnd">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isTesting==true}]]></conditionExpression>
    </sequenceFlow>
  </process>
