      <sequenceFlow id="toGtw" sourceRef="splitDown" targetRef="joinIso">
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${chkIsRunning==false}]]></conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="flow8" sourceRef="eventVMend" targetRef="messageVMfinished"></sequenceFlow>
      <serviceTask id="chkISOTask" name="Check ISO unmounted" activiti:delegateExpression="${deployCheckIso}"></serviceTask>
      <exclusiveGateway id="splitIso" name="Exclusive Gateway"></exclusiveGateway>
      <sequenceFlow id="isoRemoved" sourceRef="splitIso" targetRef="eventVMend">
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${chkIso==true}]]></conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="noIsoRemoved" sourceRef="splitIso" targetRef="joinIso">
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${chkIso==false}]]></conditionExpression>
      </sequenceFlow>
      <sequenceFlow id="toSplitIso" sourceRef="chkISOTask" targetRef="splitIso"></sequenceFlow>
      <intermediateCatchEvent id="messageVMfinished" name="MessageCatchEvent">
        <messageEventDefinition messageRef="machineInstalledId"></messageEventDefinition>
      </intermediateCatchEvent>
      <sequenceFlow id="flow7" sourceRef="messageVMfinished" targetRef="endVM"></sequenceFlow>
      <exclusiveGateway id="joinIso" name="Exclusive Gateway"></exclusiveGateway>
      <sequenceFlow id="toTimerIso" sourceRef="joinIso" targetRef="timerISO"></sequenceFlow>
      <intermediateCatchEvent id="timerDown" name="TimerCatchEvent">
        <timerEventDefinition>
          <timeDuration>PT150S</timeDuration>
        </timerEventDefinition>
      </intermediateCatchEvent>
      <sequenceFlow id="toChkDown" sourceRef="JoinDown" targetRef="chkDownVmTask"></sequenceFlow>
      <intermediateCatchEvent id="timerISO" name="TimerCatchEvent">
        <timerEventDefinition>
          <timeDuration>PT30S</timeDuration>
        </timerEventDefinition>
      </intermediateCatchEvent>
      <sequenceFlow id="toCheckISO" sourceRef="timerISO" targetRef="chkISOTask"></sequenceFlow>
      <sequenceFlow id="flow19" sourceRef="timerDown" targetRef="JoinDown"></sequenceFlow>
    </subProcess>
    <sequenceFlow id="toAcqIP" sourceRef="activateVM" targetRef="acquireIpTask"></sequenceFlow>
    <intermediateCatchEvent id="timerInstall" name="TimerCatchEvent">
      <timerEventDefinition>
        <timeDuration>PT25S</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="toCheckVM" sourceRef="timerInstall" targetRef="vmInstalledTask"></sequenceFlow>
    <intermediateCatchEvent id="timerChkIp" name="TimerCatchEvent">
      <timerEventDefinition>
        <timeDuration>PT20S</timeDuration>
